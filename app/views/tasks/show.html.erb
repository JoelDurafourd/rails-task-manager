<h1>title: <%= @task.title %></h1>
<h2>details: <%= @task.details %></h2>
<h2>completed: <input type="checkbox" <%= @task.completed ? "checked" : ""  %>></h2>
<%= link_to "tasks", "/tasks" %>
<%= link_to "edit", edit_task_path(@task) %>
<%= link_to "DESTROY THIS TASK", task_path(@task), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
